
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    
    // Nega o acesso de leitura e escrita a todos os documentos por defeito.
    // As regras explícitas para as suas coleções devem ser adicionadas abaixo.
    match /{document=**} {
      allow read, write: if false;
    }

  }
}
