rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // ATENÇÃO: Regra de depuração temporária. NÃO USE EM PRODUÇÃO.
    // Permite a leitura e escrita de todos os dados por qualquer pessoa.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
